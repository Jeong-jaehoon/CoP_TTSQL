name = "ttsql-api"
main = "src/worker.js"
compatibility_date = "2024-01-01"
account_id = "YOUR_ACCOUNT_ID"  # 실제 계정 ID로 교체

[env.production]
name = "ttsql-api-prod"

[[env.production.d1_databases]]
binding = "DB"
database_name = "ttsql-prod"
database_id = "YOUR_PRODUCTION_DB_ID"  # 실제 DB ID로 교체

[env.development]
name = "ttsql-api-dev"

[[env.development.d1_databases]]
binding = "DB"
database_name = "ttsql-dev" 
database_id = "YOUR_DEVELOPMENT_DB_ID"  # 실제 DB ID로 교체

[env.local]
name = "ttsql-api-local"

[[env.local.d1_databases]]
binding = "DB"
database_name = "ttsql-local"
database_id = "YOUR_LOCAL_DB_ID"  # 실제 DB ID로 교체